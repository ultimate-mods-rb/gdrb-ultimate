;instrument icons
{set $guitaricon g}
{set $drumicon d}
{set $vocalsicon v}
{set $bassicon b}

;sets default track speed. 1.15 is 115%, 1.30 is 130% and so on.
{set $trackspeed 1.0}

;sets default song speed 1.15 is 115%, 1.30 is 130% and so on.
{set $speedmod 1.0}

;set default venue fps.
{set $emulatefps 30}

;no venues
{set $blackbg FALSE}

;song title always on
{set $songtitle FALSE}

;show artist text
{set $showartist FALSE}

;force show artist text for non-gd songs ONLY
{set $forceartist TRUE}

;disables the background graphics. only works on wii and does nothing if set to true by default
{set $disablegfx FALSE}

;enables track muting by default
{set $trkmuting FALSE}

;enables whammy pitch bending by default
{set $whammypb TRUE}

;sync dificulty speeds
{set $syncdifspeeds FALSE}

;legacy bass streak
{set $legacybass FALSE}

;skip intro by default
{set $skipintro TRUE}

;dont skip splash by default
{set $skipsplash FALSE}

;dont force outfit by default
{set $advforceoutfit FALSE}

;dont force venue by default
{set $forcevenue "off"}

;default outfits/insts for adv force outfit
{set $bjoutfit dookie}
{set $mdoutfit dookie}
{set $tcoutfit dookie}
{set $bjinst1 guitar_blue01}
{set $mdinst1 bass_lh_ricken_4001s_fireglo}
{set $tcinst1 drum_dw}

;default gem/smasher/sustain colors
{set $gtrgem1 gem_red.wid}
{set $gtrhgem1 gem_red_hopo.wid}
{set $gtrgem2 gem_orange.wid}
{set $gtrhgem2 gem_orange_hopo.wid}
{set $gtrgem3 gem_yellow.wid}
{set $gtrhgem3 gem_yellow_hopo.wid}
{set $gtrgem4 gem_green.wid}
{set $gtrhgem4 gem_green_hopo.wid}
{set $gtrgem5 gem_blue.wid}
{set $gtrhgem5 gem_blue_hopo.wid}
{set $drmgem1 gem_kick.wid}
{set $drmgem2 drum_red.wid}
{set $drmgem3 drum_yellow.wid}
{set $drmgem4 drum_blue.wid}
{set $drmgem5 drum_green.wid}
{set $gtrsm1 red}
{set $gtrsm2 orange}
{set $gtrsm3 yellow}
{set $gtrsm4 green}
{set $gtrsm5 blue}
{set $gtrsus1 red}
{set $gtrsus2 orange}
{set $gtrsus3 yellow}
{set $gtrsus4 green}
{set $gtrsus5 blue}
{set $drms1 red}
{set $drms2 yellow}
{set $drms3 blue}
{set $drms4 green}

;default mic gains
{set $mic_gain_1 4}
{set $mic_gain_2 4}
{set $mic_gain_3 4}

;gem texture
{set $gem "none"}

;smasher texture
{set $smasher "none"}

;streak texture
{set $streak "none"}

;overdrive texture
{set $spotlight "none"}

;multiplier ring texture
{set $multiring "none"}

;overdrive bar texture
{set $odbar "none"}

;score texture
{set $scoretex "none"}

;custom track scaling/angles/rotation
;highly experimental, don't edit unless you know what you're doing
{set $trackangle FALSE}

;track angle presets that closely match the source games
;available options: "gdrb", "rb2", "rb4", "rr" "none"
{set $anglepreset "gdrb"}
;set to "none" if you want to manually edit the numbers below

;the default angles set here is rb2's
{set $track_pitch -44.5} ;camera pitch
{set $track_fov 21.0515995025635} ;field of view of the track camera
{set $track_y_pos -83.5} ;y position of the camera (in/out)
{set $track_z_pos 18.5} ;z position of the camera (up/down)
{set $track_fade_start 60} ;start point of the fadeout at the end of the track
{set $track_fade_length 40} ;length of the fadeout at the end of the track

{platform_mgr set_notify_ui_location kOSNotifyBottomCenter}
{set $saveload_skip_upload FALSE}

#define GDRBU
(1)

