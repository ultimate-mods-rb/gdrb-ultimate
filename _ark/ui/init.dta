;modifier funcs
{if $postfx
   #ifdef HX_WII {pp_override} #else {rnd toggle_all_postprocs} #endif}
{if $padtar
   #ifdef HX_PS3
   {set_elem {find $syscfg joypad instrument_mapping dualshock} 1 kControllerGuitar}
   #endif
   {set_elem {find $syscfg joypad instrument_mapping analog} 1 kControllerGuitar}}
{if $paddrum
   #ifdef HX_PS3
   {set_elem {find $syscfg joypad instrument_mapping dualshock} 1 kControllerDrum}
   #endif
   {set_elem {find $syscfg joypad instrument_mapping analog} 1 kControllerDrum}}

{unless {== $gem none} {set $gemcustomtexture TRUE}}

{unless {== $smasher none} {set $smashercustomtexture TRUE}}

{unless {== $spotlight none} {set $spotlightcustomtexture TRUE}}

{unless {== $streak none} {set $streakcustomtexture TRUE}}

{unless {== $multiring none} {set $multiringcustomtexture TRUE}}

{unless {== $odbar none} {set $odbarcustomtexture TRUE}}

{unless {== $scoretex none} {set $scorecustomtexture TRUE}}

{if {== $anglepreset gdrb}
   {set $trackangle FALSE}}
{if {== $anglepreset rb2}
      {set $trackangle TRUE}
      {set $track_pitch -44.5}
      {set $track_fov 21.0515995025635}
      {set $track_y_pos -83.5}
      {set $track_z_pos 18.5}
      {set $track_fade_start 60}
      {set $track_fade_length 40}}
{if {== $anglepreset rb4}
      {set $trackangle TRUE}
      {set $track_pitch -40.5}
      {set $track_fov 21}
      {set $track_y_pos -112.5}
      {set $track_z_pos 26.51}
      {set $track_fade_start 90}
      {set $track_fade_length 30}}
{if {== $anglepreset rr}
      {set $trackangle TRUE}
      {set $track_pitch -90}
      {set $track_fov 19}
      {set $track_y_pos -165}
      {set $track_z_pos 36}
      {set $track_fade_start 15}
      {set $track_fade_length 45}}

{profile_mgr set_mic_gain 0 $mic_gain_1} {profile_mgr update_mic_gain 0}

{profile_mgr set_mic_gain 1 $mic_gain_2} {profile_mgr update_mic_gain 1}

{profile_mgr set_mic_gain 2 $mic_gain_3} {profile_mgr update_mic_gain 2}
